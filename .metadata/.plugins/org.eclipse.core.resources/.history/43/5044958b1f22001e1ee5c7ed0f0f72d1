package programacionOrientadaObjetos;

public class TestReferencia1 {
	public static void main(String[] args) {
		Cliente juan = new Cliente();
		juan.nombre = "Juan";
		juan.direccion = "Alo svossdao Blv.";
		juan.documento = "452894201";
		juan.telefono = "0929842";
		
		Cuenta cuentaJuan = new Cuenta();
		cuentaJuan.titular = juan;
		cuentaJuan.saldo = 900;
		
		System.out.println("Nombre: "+cuentaJuan.titular.nombre);
		System.out.println("Documento: "+cuentaJuan.titular.documento);
		System.out.println("Saldo: "+cuentaJuan.saldo);
	}
}
